{
	"name": "svelte-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --port 3333 --host",
		"build": "vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"husky:install": "cd ../.. && husky install app/svelte/.husky",
		"prepare": "svelte-kit sync && npm run husky:install",
		"postinstall": "npm run husky:install",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "^0.1.1",
		"@esbuild-plugins/node-modules-polyfill": "^0.1.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"buffer": "^6.0.3",
		"esbuild": "^0.14.49",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"postcss": "^8.4.14",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"process": "^0.11.10",
		"rollup-plugin-node-builtins": "^2.1.2",
		"rollup-plugin-node-polyfills": "^0.2.1",
		"svelte": "^3.49.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^2.9.13"
	},
	"type": "module",
	"dependencies": {
		"@inrupt/solid-client": "^1.23.1",
		"@inrupt/solid-client-authn-browser": "^1.12.1",
		"@inrupt/vocab-common-rdf": "^1.0.5",
		"@js-temporal/polyfill": "^0.4.1",
		"@sveltejs/adapter-static": "^1.0.0-next.35",
		"@tailwindcss/line-clamp": "^0.4.0",
		"svelte-hero-icons": "^4.1.3"
	},
	"license": "MIT"
}
